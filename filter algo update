func filter() -> [Restaurant] {
  var typeFilters = []
  var priceFilters = []
  var townFilters = []
  var typeRes: [Restaurant] = []
  var priceRes: [Restaurant] = []
  var townRes: [Restaurant] = []
  var tempList = [Restaurant]()
  for fil in selectedFilters {
    if fil.contains("$") {
      priceFilters.append(fil)
    } else if fil.isEqual ("Rye" || "Greenwich" || "Port Chester" ... {
      townFilters.append(fil)
    } else {
      typeFilters.append(fil)
    }
  }
  tempList2 = allRestaurants //copy of all restaurants
  
  for type in typeFilters {
	  tempList += tempList2.filter() {$0.type.contains {$0.contains(type)}}
  }
  tempList2 = tempList
  tempList = []
  for price in priceFilters {
	  tempList += tempList2.filter() {$0.price.character.count == type.character.count}
  }
  tempList2 = tempList
  tempList = []
  for town in townFilters {
	  tempList += tempList2.filter() {$0.town.contains(town)}
  }
  
  
  filteredList = []
  for res in tempList {
	  if !filteredList.contains(res) {
		  filteredList.append(res)
	  }
  }
  
  return filteredList
  return tempList
}
